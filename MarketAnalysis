<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<style type="text/css" media="screen, print">
body {font:normal 12px Arial,sans-serif; background-color:"#FBFBFB";}
</style>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load('current', {'packages':['line']});
      google.charts.setOnLoadCallback(drawChart1);
google.charts.setOnLoadCallback(drawChart2);
google.charts.setOnLoadCallback(drawChart3);
google.charts.setOnLoadCallback(drawChart4);

    function drawChart1() {

      var data1 = new google.visualization.DataTable();
#chartcolumn1

      data1.addRows([
      #chartdata1
      ]);


      var options = {
        chart: {
          title: '#charttitle1',
          interpolateNulls: true
        },
        width: 1200,
        height: 500
      };
      var chart1 = new google.charts.Line(document.getElementById('chart1'));


      chart1.draw(data1, options);

}

function drawChart2() {

      var data2 = new google.visualization.DataTable();
#chartcolumn2

      data2.addRows([
      #chartdata2
      ]);
      var options = {
        chart: {
          title: '#charttitle2',
          interpolateNulls: true
        },
        width: 1500,
        height: 500
      };
 
var chart2 = new google.charts.Line(document.getElementById('chart2'));

chart2.draw(data2, options);

}

function drawChart3() {



      var data3 = new google.visualization.DataTable();
#chartcolumn3

      data3.addRows([
      #chartdata3
      ]);

      var options = {
        chart: {
          title: '#charttitle3',
          interpolateNulls: true
        },
        width: 1500,
        height: 500
      };

var chart3 = new google.charts.Line(document.getElementById('chart3'));

chart3.draw(data3, options);
}

    function drawChart4() {

      var data4 = new google.visualization.DataTable();
#chartcolumn4

      data4.addRows([
      #chartdata4
      ]);


      var options = {
        chart: {
          title: '#charttitle4',
          interpolateNulls: true
        },
        width: 1200,
        height: 500
      };
      var chart4 = new google.charts.Line(document.getElementById('chart4'));


      chart4.draw(data4, options);

}
    </script>
</head>
