        'Debug.Print rs.RecordCount
        If rs.RecordCount = 0 Then
            Exit Sub
        End If
        
        rs.MoveFirst
        Do While Not rs.EOF
            'mtime = CStr(rs.Fields(eventtime).Value)
            'mtime = Left(mtime, 5)
            'Debug.Print rate * 60
            es = rs.Fields(eventtime).Value
            'es = ct(mtime)
